@model Contact
@{
    ViewData["Title"] = "Detail";
}
@section Css{
    <link href="~/Admin/build/css/custom.min.css" rel="stylesheet">
}
<section class="pcoded-main-container">
    <div class="pcoded-content">
        <div class="row">
            <div class="col-12">
                <form asp-action="Detail">
                    <div class="form-group">
                        <label>Name</label>
                        <input disabled class="form-control" asp-for="Name">
                    </div>
                    <div class="form-group">
                        <label>Phone</label>
                        <input disabled class="form-control" asp-for="Phone">
                    </div>
                    <div class="form-group">
                        <label>Email</label>
                        <input disabled class="form-control" asp-for="Email">
                    </div>
                    <div class="form-group">
                        <label>Message</label>
                        <input disabled class="form-control" asp-for="Message">
                    </div>
                    <a asp-action="Index" class="btn btn-success"><i class="feather mr-2 icon-check-circle"></i>Go to list</a>
                </form>
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <script src="~/Admin/src/js/index.js"></script>
}






